{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "telemetry": {
    "serviceName": "TELEMETRY_SERVICE_NAME",
    "hostname": "TELEMETRY_HOST_NAME",
    "version": "TELEMETRY_SERVICE_VERSION",
    "logger": {
      "level": "LOG_LEVEL",
      "prettyPrint": {
        "__name": "LOG_PRETTY_PRINT_ENABLED",
        "__format": "boolean"
      }
    },
    "tracing": {
      "enabled": "TELEMETRY_TRACING_ENABLED",
      "url": "TELEMETRY_TRACING_URL"
    },
    "metrics": {
      "enabled": "TELEMETRY_METRICS_ENABLED",
      "url": "TELEMETRY_METRICS_URL",
      "interval": "TELEMETRY_METRICS_INTERVAL"
    }
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "redis": {
    "host": "REDIS_HOST",
    "port": {
      "__name": "REDIS_PORT",
      "__format": "number"
    },
    "username": "REDIS_USERNAME",
    "password": "REDIS_PASSWORD",
    "enableSslAuth": {
      "__name": "REDIS_ENABLE_SSL_AUTH",
      "__format": "boolean"
    },
    "sslPaths": {
      "ca": "REDIS_CA_PATH",
      "key": "REDIS_KEY_PATH",
      "cert": "REDIS_CERT_PATH"
    },
    "database": {
      "__name": "REDIS_DATABASE",
      "__format": "number"
    }
  },
  "kafka": {
    "brokers": "KAFKA_BROKERS",
    "clientId": "KAFKA_CLIENT_ID",
    "enableSslAuth": {
      "__name": "KAFKA_ENABLE_SSL_AUTH",
      "__format": "boolean"
    },
    "sslPaths": {
      "ca": "KAFKA_CA_PATH",
      "key": "KAFKA_KEY_PATH",
      "cert": "KAFKA_CERT_PATH"
    }
  },
  "kafkaConsumer": {
    "groupId": "KAFKA_CONSUMER_GROUP_ID"
  },
  "kafkaTopics": {
    "input": {
      "__name": "KAFKA_INPUT_TOPIC",
      "__format": "json"
    }
  },
  "elastic": {
    "node": "ELASTIC_URL",
    "auth": {
      "username": "ELASTIC_USERNAME",
      "password": "ELASTIC_PASSWORD"
    },
    "properties": {
      "index": "ELASTIC_INDEX"
    }
  },
  "spike": {
    "url": "SPIKE_URL",
    "auth": {
      "username": "SPIKE_USERNAME",
      "password": "SPIKE_PASSWORD"
    },
    "requestBody": {
      "__name": "SPIKE_REQUEST_BODY",
      "__format": "json"
    }
  },
  "secondarySpike": {
    "url": "SECOND_SPIKE_URL",
    "auth": {
      "username": "SECOND_SPIKE_USERNAME",
      "password": "SECOND_SPIKE_PASSWORD"
    },
    "requestBody": {
      "__name": "SECOND_SPIKE_REQUEST_BODY",
      "__format": "json"
    }
  },
  "application": {
    "userDataService": {
      "endpoint": "USER_DATA_SERVICE_ENDPOINT",
      "headers": {
        "__name": "HEADER_PARAMS",
        "__format": "json"
      },
      "queryParams": {
        "__name": "USER_DATA_SERVICE_QUERY_PARAMS",
        "__format": "json"
      },
      "domain": {
        "__name": "USER_DATA_SERVICE_DOMAIN",
        "__format": "json"
      }
    }
  }
}
