{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "redis": {
    "host": "redis",
    "port": 6379,
    "username": "",
    "password": "",
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    },
    "database": 1,
    "prefix": "userdata"
  },
  "kafka": {
    "brokers": "kafka:9092",
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    }
  },
  "kafkaConsumer": {
    "groupId": "123"
  },
  "kafkaTopics": {
    "input": ["topic"]
  },
  "elastic": {
    "node": "http://elasticsearch:9200",
    "auth": {
      "username": "elastic",
      "password": "changeme"
    },
    "requestTimeout": 60000,
    "properties": {
      "index": "enrich_index"
    }
  },
  "spike": {
    "url": "http://spike/token",
    "auth": {
      "username": "spike",
      "password": "spike"
    },
    "requestBody": {
      "body": true
    }
  },
  "secondarySpike": {
    "url": "http://spike/token",
    "auth": {
      "username": "secondSpike",
      "password": "secondSpike"
    },
    "requestBody": {
      "body": true
    }
  },
  "application": {
    "userDataService": {
      "serviceName": "kartoffel",
      "endpoint": "http://userDataService/kartoffel",
      "queryParams": {
        "extraDetails": true
      },
      "headers": {
        "headerDetails": true
      },
      "domain": "local"
    }
  }
}
