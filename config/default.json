  },
  "kafkaConsumer": {
    "groupId": "123"
  },
  "kafkaTopics": {
    "input": ["topic"]
  },
  "elastic": {
    "node": "https://vector-elasticsearch-vector-dev.apps.j1lk3njp.eastus.aroapp.io",
    "auth": {
      "username": "elastic",
      "password": "changeme"
    },
    "requestTimeout": 60000,
    "properties": {
      "index": "enrich_index"
    }
  },
  "application": {
    "userDataService": {
      "endpoint": "http://localhost/mirage",
      "queryParams": {
        "extraDetails": true
      },
      "headers": {
        "headerDetails": true
      }
    }
  }
